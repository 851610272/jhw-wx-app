/* 产品列表 */
.cells{display:flex;flex-wrap:wrap;justify-content:space-between;padding-bottom:1vh}
.cell{display:flex;width:49vw;box-sizing:border-box;padding-top:1.5vh}
.cell:nth-child(odd){justify-content:flex-start}
.cell:nth-child(even){justify-content:flex-end}
.cell_bd{overflow:hidden}
.pro_img{width:376rpx;}
.pro_img image{max-width: 100%;max-height: 100%;}
.pro_title{padding:5px 5px 0 5px}
.pro_price{padding:5px}
/* 搜索 */
.search{background:#ff6214;padding:7px 0 7px 10px;overflow:hidden}
.search .search-btn{background:#fff;color:#999;font-size:12px;text-align:center;height:26px;line-height:26px;border-radius:13px;width:630rpx;float:left}
.search .search-btn:active{background:#fff5f0}
.search image{width:18px;height:18px;vertical-align:middle}
.search .category{padding:4px 15px;float:right}
